package excelr;

public class HelloWorld 
{
	public static void main(String[] args) 
	{
		test(); //how to debug
		System.out.println("Hello World!"); 
	}
	static void test();
	{
		for (int i = 0; i <= 5; i++)
		{
			System.out.println("Iteration: "  + i);
		}
	}
}


//main ctrl+space
//syso + ctrl space
//ctrl + F11 to run
